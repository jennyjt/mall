<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zsbatech.baasKettleManager.dao.FilesFileCatalogVOMapper">
    <resultMap id="BaseResultMap" type="com.zsbatech.baasKettleManager.vo.FilesFileCatalogVO">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="file_id" property="fileId" jdbcType="INTEGER"/>
        <result column="file_catalog_id" property="fileCatalogId" jdbcType="INTEGER"/>
    </resultMap>
    <sql id="Base_Column_List">
    id, file_id, file_catalog_id
  </sql>

    <insert id="insert" parameterType="com.zsbatech.baasKettleManager.vo.FilesFileCatalogVO">
    insert into files_file_catalog (id, file_id, file_catalog_id)
    values (#{id,jdbcType=INTEGER}, #{fileId,jdbcType=INTEGER},#{fileCatalogId,jdbcType=INTEGER})
    </insert>

    <insert id="insertBatch" parameterType="java.util.List">
        insert into files_file_catalog (id, file_id, file_catalog_id)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            ( #{item.id,jdbcType=INTEGER}, #{item.fileId,jdbcType=INTEGER},#{item.fileCatalogId,jdbcType=INTEGER})
        </foreach>
    </insert>
    <select id="queryByFileId" resultMap="BaseResultMap" parameterType="java.util.List">
    select
    <include refid="Base_Column_List"/>
    from files_file_catalog
    where file_id = #{id,jdbcType=INTEGER}
    </select>

</mapper>